{
  "name": "icare-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "start:frontend": "react-router dev",
    "start:backend": "json-server --watch db.json --port 4000",
    "dev": "npm-run-all --parallel start:frontend start:backend",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@remix-run/node": "^2.17.1",
    "@stencil/react-output-target": "^1.2.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-library": "^0.1.0",
    "react-pdf": "^10.1.0",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler"
  }
